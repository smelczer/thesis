<Group hide-input="false" hide-output="false" labelreference="L367" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"><Font encoding="UTF-8"># Create the matrix \134tilde{H} and find its determinant
</Font>Ht := Matrix(nn-1,nn-1):

for i from 1 to nn do
 for j from 1 to nn do
  U[i,j] := varsx[i]*varsx[j]*diff(H,varsx[i],varsx[j]);
od:od:

for i from 1 to nn-1 do
 for j from 1 to nn-1 do
  if (i &lt;&gt; j) then Ht[i,j] := x*diff(H,x) + (U[i,j]-U[i,nn]-U[j,nn]+U[nn,nn]);
  else Ht[i,j] := 2*x*diff(H,x) + (U[i,i]-2*U[i,nn]+U[nn,nn]); fi:
od:od:

Htdet := Determinant(Ht);</Text-field>
</Input>
<Output><Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display="">LCgqJEkieEc2IiIiIyIiJyokSSJ5R0YlRiYiIyEpRiQhI1U=</Equation></Text-field>
</Output>
</Group>
<Group hide-output="false" labelreference="L381" drawlabel="true">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># Add -G = -1, x*y, Htdet into the Kronecker representation
GB2 := Groebner[Basis]([P,op(remove(has,GB,t)),T - x*y,ht - Htdet, g+1],plex(g,T,ht,lambda,x,y,u)):
for v in [g,T,ht] do
  EQ := op(select(has,GB2,v));
  Q[v] := rem(Pd*solve(EQ,v), P, u):
od:</Text-field>
</Input>
</Group>
